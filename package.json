{
  "name": "workflow-thingy",
  "version": "0.1.0",
  "scripts": {
    "bundle": "npx webpack --config webpack.config.js",
    "build": "build --win --x64 --mirror https://cnpmjs.org/mirrors/nwjs/ src/"
  },
  "keywords": [],
  "author": "Nathan Jones",
  "license": "ISC",
  "build": {
    "appId": "au.bluefox.workflow",
    "nwVersion": "0.50.2",
    "packed": true,
    "targets": [
      "zip",
      "nsis7z"
    ],
    "strippedProperties": [
      "build"
    ],
    "outputPattern": "${NAME} ${VERSION} ${PLATFORM} ${ARCH}",
    "win": {
      "productName": "Workflow Thingy",
      "companyName": "BlueFox.au",
      "copyright": "copyright",
      "icon": "./assets/logo.ico"
    },
    "nsis": {
      "installDirectory": "$PROGRAMFILES\\${_COMPANYNAME}\\${_APPNAME}",
      "diffUpdaters": true
    }
  },
  "description": "A thingy for workflow",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "nwjs-builder-phoenix": "^1.15.0",
    "nwjs-types": "^1.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "constants-browserify": "^1.0.0"
  }
}
